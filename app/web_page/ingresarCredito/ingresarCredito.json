    "submit_errors_list" : {
      "type" : "expression",
      "value" : [
        "if($data.formOutput && $data.formOutput._submitError && $data.formOutput._submitError.explanations){",
        "\tconst liElements = $data.formOutput._submitError.explanations",
        "\t\t.filter(cause => cause !== null)",
        "\t\t.map(cause => \"<li>\" + cause + \"</li>\")",
        "\t\t.join('');",
        "\tif(liElements){",
        "\t\treturn \"<ul>\" + liElements + \"</ul>\";",
        "\t}",
        "}"
      ],
      "exposed" : false
    },
    "formOutput" : {
      "type" : "expression",
      "value" : [
        "if( $data.contrato && $data.credito ){",
        "\treturn {",
        "\t\t//map contrato variable to expected task contract input",
        "\t\tcontratoInput: {",
        "\t\t\ttipoContrato: 'Solicitud de credito',",
        "\t\t\tsolicitante: $data.credito.cliente !== undefined ? $data.credito.cliente : null,",
        "\t\t\tfechaContrato: $data.credito.fechaPrestamo !== undefined ? $data.credito.fechaPrestamo : null,",
        "\t\t\tmonto: $data.credito.montoPrestado !== undefined ? $data.credito.montoPrestado : null",
        "\t\t},",
        "\t\t//map cuenta variable to expected task contract input",
        "\t\tcreditoInput: {",
        "\t\t\tmontoPrestado: $data.credito.montoPrestado !== undefined ? $data.credito.montoPrestado : null,",
        "\t\t\tfechaPrestamo: $data.credito.fechaPrestamo !== undefined ? $data.credito.fechaPrestamo : null,",
        "\t\t\tnroCuotas: $data.credito.nroCuotas !== undefined ? $data.credito.nroCuotas : null",
        "\t\t}",
        "\t}",
        "}"
      ],
      "exposed" : false
    },
