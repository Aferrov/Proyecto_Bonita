      "type" : "expression",
      "value" : [
        "if($data.formOutput && $data.formOutput._submitError && $data.formOutput._submitError.explanations){",
        "\tconst liElements = $data.formOutput._submitError.explanations",
        "\t\t.filter(cause => cause !== null)",
        "\t\t.map(cause => \"<li>\" + cause + \"</li>\")",
        "\t\t.join('');",
        "\tif(liElements){",
        "\t\treturn \"<ul>\" + liElements + \"</ul>\";",
        "\t}",
        "}"
      ],
      "exposed" : false
    },
    "formOutput" : {
      "type" : "expression",
      "value" : [
        "if ($data.cuenta && $data.contrato) {\r",
        "  const payload = {\r",
        "    cuentaInput: {\r",
        "      fechaApertura: $data.cuenta.fechaApertura || null,\r",
        "      saldo: $data.cuenta.saldo || null\r",
        "    },\r",
        "    contratoInput: {\r",
        "      tipoContrato: 'Apertura de cuenta',\r",
        "      solicitante: $data.cuenta.cliente || null,\r",
        "      fechaContrato: $data.cuenta.fechaApertura || null,\r",
        "      monto: $data.cuenta.saldo || null\r",
        "    }\r",
        "  };\r",
        "  console.log(\"Datos enviados al contrato:\", payload);\r",
        "  return payload;\r",
        "}\r"
      ],
      "exposed" : false
    },
