    "submit_errors_list" : {
      "type" : "expression",
      "value" : [
        "if($data.formOutput && $data.formOutput._submitError && $data.formOutput._submitError.explanations){",
        "\tconst liElements = $data.formOutput._submitError.explanations",
        "\t\t.filter(cause => cause !== null)",
        "\t\t.map(cause => \"<li>\" + cause + \"</li>\")",
        "\t\t.join('');",
        "\tif(liElements){",
        "\t\treturn \"<ul>\" + liElements + \"</ul>\";",
        "\t}",
        "}"
      ],
      "exposed" : false
    },
    "formOutput" : {
      "type" : "expression",
      "value" : [
        "if( $data.contrato && $data.cuenta ){",
        "\treturn {",
        "\t\t//map contrato variable to expected task contract input",
        "\t\tcontratoInput: {",
        "\t\t\ttipoContrato: 'Apertura de cuenta',",
        "\t\t\tsolicitante: $data.cuenta.cliente !== undefined ? $data.cuenta.cliente : null,",
        "\t\t\tfechaContrato: $data.cuenta.fechaApertura !== undefined ? $data.cuenta.fechaApertura : null,",
        "\t\t\tmonto: $data.cuenta.saldo !== undefined ? $data.cuenta.saldo : null",
        "\t\t},",
        "\t\t//map cuenta variable to expected task contract input",
        "\t\tcuentaInput: {",
        "\t\t\tfechaApertura: $data.cuenta.fechaApertura !== undefined ? $data.cuenta.fechaApertura : null,",
        "\t\t\tsaldo: $data.cuenta.saldo !== undefined ? $data.cuenta.saldo : null",
        "\t\t}",
        "\t}",
        "}"
      ],
      "exposed" : false
    },
